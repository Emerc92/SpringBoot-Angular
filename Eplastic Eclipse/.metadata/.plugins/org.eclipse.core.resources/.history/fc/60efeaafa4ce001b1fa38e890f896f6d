#server config
server.port=9090

#database config
spring.datasource.url=jdbc:mysql://localhost:3306/eplasticorders?useSSL=false
spring.datasource.username=root
spring.datasource.password=root
#update configuration creates tables too.
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

#hibernate config
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5Dialect

#security
jwt.secret=secret
jwt.expiration=3600