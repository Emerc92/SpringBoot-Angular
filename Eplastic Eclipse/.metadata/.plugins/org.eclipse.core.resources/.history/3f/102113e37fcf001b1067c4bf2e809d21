package it.wibs.eplastic.repository;

import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;

import it.wibs.eplastic.entities.CommissionsEntity;

public interface CommissionsRepo extends JpaRepository<CommissionsEntity, String>{

	Optional<CommissionsEntity> findById(String commissionId);
	boolean existsById(String commissionId);
}
